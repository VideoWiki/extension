{
    "manifest_version": 2,
    "name": "HTML Appender",
    "version": "1.0",
    "description": "Appends HTML to web pages",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "vlogo.png",
            "48": "vlogo.png",
            "128": "vlogo.png"
        }
    },
    "icons": {
        "16": "vlogo.png",
        "48": "vlogo.png",
        "128": "vlogo.png"
    }
}