{
  "manifest_version": 3,
  "name": "VideoWiki Uploaderds",
  "version": "1.0",
  "description": "VideoWiki Uploader",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "vlogo.png",
      "48": "vlogo.png",
      "128": "vlogo.png"
    }
  },
  "icons": {
    "16": "vlogo.png",
    "48": "vlogo.png",
    "128": "vlogo.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}